  "bugs": {
    "url": "https://github.com/filipecsoares/api-node/issues"
  },
  "homepage": "https://github.com/filipecsoares/api-node#readme",
  "devDependencies": {
    "husky": "^4.2.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "validator": "^12.2.0"
  }
}
